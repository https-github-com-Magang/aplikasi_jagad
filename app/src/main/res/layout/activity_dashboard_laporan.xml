<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_jagad2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Jagad"
        android:textColor="#FF7629"
        android:textSize="36sp"
        app:layout_constraintHorizontal_bias="0.085"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_sewaMotor"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="108dp"
        android:background="@drawable/background_putih"
        android:shadowColor="@color/black"
        android:text=" Sewa Motor"
        android:textSize="18sp"
        app:cornerRadius="15dp"
        app:layout_constraintStart_toStartOf="@+id/tv_jagad2"
        app:layout_constraintTop_toBottomOf="@+id/tv_jagad2" />

    <Button
        android:id="@+id/btn_sewaMobil"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="108dp"
        android:background="@drawable/background_putih"
        android:shadowColor="@color/black"
        android:text="Sewa Mobil"
        android:textSize="18sp"
        app:cornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintStart_toEndOf="@+id/btn_sewaMotor"
        app:layout_constraintTop_toBottomOf="@+id/tv_jagad2" />

    <Button
        android:id="@+id/btn_loketSewa"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/background_putih"
        android:shadowColor="@color/black"
        android:text=" Loket"
        android:textSize="18sp"
        app:cornerRadius="15dp"
        app:layout_constraintStart_toStartOf="@+id/btn_sewaMotor"
        tools:layout_editor_absoluteY="382dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_hlmKurir"
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:background="@drawable/background_putih"
        android:shadowColor="@color/black"
        android:text="Kurir"
        android:textSize="18sp"
        app:cornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="@+id/btn_sewaMobil"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/btn_loketSewa"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="382dp" />

    <LinearLayout
        android:layout_width="430dp"
        android:layout_height="55dp"
        android:background="@drawable/background_putih"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0dp"
            app:srcCompat="@drawable/homeicon" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="105dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0dp"
            app:srcCompat="@drawable/list" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0dp"
            app:srcCompat="@drawable/usernon" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>