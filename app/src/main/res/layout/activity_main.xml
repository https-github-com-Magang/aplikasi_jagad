<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tv_jagad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Jagad"
        android:textColor="#FF7629"
        android:textSize="36sp"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_kurir"
        android:layout_width="260dp"
        android:layout_height="39dp"
        android:layout_marginTop="380dp"
        android:background="@drawable/background_orange"
        android:fontFamily="@font/lato_bold"
        android:text="Kurir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_jagad" />

    <Button
        android:id="@+id/btn_admin"
        android:layout_width="260dp"
        android:layout_height="39dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/background_orange"
        android:fontFamily="@font/lato_bold"
        android:text="Admin"
        app:layout_constraintEnd_toEndOf="@+id/btn_kurir"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/btn_kurir"
        app:layout_constraintTop_toBottomOf="@+id/btn_kurir" />


</androidx.constraintlayout.widget.ConstraintLayout>